{
  "name": "driving-license-tracker",
  "version": "1.0.0",
  "description": "Driving License Tracking System - Monorepo",
  "main": "index.js",
  "scripts": {
    "build": "cd frontend && npm install && npm run build",
    "start-backend": "cd Backend && npm install && node server.js",
    "start-frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "keywords": [
    "driving-license",
    "tracking",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  },
  "workspaces": [
    "frontend",
    "Backend"
  ]
}
